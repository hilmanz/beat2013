</div><!--end.section-->

<script type="text/javascript">

		var data = {$loginData};
	
{literal}	
$(function () {

	console.log(data);
	var tesData = new Array();
	var labelData = new Array();		
		
    for (var dataDate in data){
			tesData.push(parseInt(data[dataDate].total)); // coba['2013-01-01']['total']
			labelData.push(String(dataDate));
		}
	    
	gethighChart('line','test  Chart','visit',tesData,labelData,1,true);
	gethighChart('line','test  Chart','pageview',tesData,labelData,1,true);
	gethighChart('line','test  Chart','avgVisit',tesData,labelData,1,true);
	gethighChart('line','test  Chart','avgTimeonpage',tesData,labelData,1,true);
	gethighChart('line','test  Chart','bounceRate',tesData,labelData,1,true);
	
	//drillDownChart('column','test Chart','container',dataCoba,cateActivityGroup,1 ,false);
	});
		
</script>
{/literal}